[alias]
        tree = log --graph --abbrev-commit --decorate --format=format:'%C(bold cyan)%h%C(reset) %C(green)(%ar)%C(reset) %C(dim white)- %an%C(reset) %C(auto)%d%C(reset)\n   â†³ %C(reset)%s%C(reset)'
